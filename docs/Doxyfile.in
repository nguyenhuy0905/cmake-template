PROJECT_NAME = "@PROJECT_NAME@"
PROJECT_NUMBER = "@PROJECT_VERSION@"

INPUT = "@PROJECT_SOURCE_DIR@" "@PROJECT_SOURCE_DIR@/src"
EXTRACT_ALL = YES
EXCLUDE_PATTERNS = "@PROJECT_SOURCE_DIR@/conanfile.py" "@PROJECT_SOURCE_DIR@/*.test.*" "@PROJECT_BINARY_DIR@" "@PROJECT_SOURCE_DIR@/build"
RECURSIVE = YES
OUTPUT_DIRECTORY = "@DOXYGEN_OUTPUT_DIRECTORY@"
USE_MDFILE_AS_MAINPAGE = "@PROJECT_SOURCE_DIR@/README.md"
FULL_PATH_NAMES = YES
STRIP_FROM_PATH = "@PROJECT_SOURCE_DIR@/include" "@PROJECT_SOURCE_DIR@"
